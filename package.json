{
  "name": "vuesic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start": "node server.js"
  },
  "dependencies": {
    "cloudinary-core": "^2.5.0",
    "connect-history-api-fallback": "^1.5.0",
    "express": "^4.16.3",
    "font-awesome": "^4.7.0",
    "hls.js": "^0.9.1",
    "lodash": "^4.17.10",
    "rollup": "^0.59.2",
    "serve-static": "^1.13.2",
    "vue": "^2.5.16",
    "vue-aplayer": "^1.5.2",
    "vue-router": "^3.0.1",
    "vue-social-sharing": "^2.3.3",
    "vue-spinner": "^1.0.3",
    "vuetify": "^1.0.16"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-beta.10",
    "@vue/cli-plugin-eslint": "^3.0.0-beta.10",
    "@vue/cli-service": "^3.0.0-beta.10",
    "vue-cli-plugin-vuetify": "^0.1.3",
    "vue-template-compiler": "^2.5.13"
  },
  "babel": {
    "presets": [
      [
        "@vue/app",
        {
          "useBuiltIns": "entry"
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2017
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "heroku-run-build-script": true
}
